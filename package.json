{   
    "name": "sake",
    "description": "A Rake-like application for managing builds and for Stitching JavaScript, CSS, HTML, and other sundry files together for web development.",
    "version": "0.0.4",
    "keywords": ["packager", "css", "javascript", "html", "web", "development"],
    "main": "./lib/app.js",
    "bin": {
        "sake": "./bin/sake"
    },
    "directories": {
        "lib": "lib"
    },
    "dependencies": {
        "proteus":  ">=0.0.x",
        "nomnom":   ">=1.5.x",
        "glob":     ">=2.1.0"
    },
    "devDependencies": {
        "should": "*"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "http://github.com/jhamlet/sake-node/raw/master/LICENSE"
        }
    ],
    "repository": {
        "type": "git",
        "url": "http://github.com/jhamlet/sake-node.git"
    },
    "preferGlobal": true
}