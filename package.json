{   
    "name": "stitch",
    "description": "Compose JavaScript, CSS, HTML, and other sundry files for web development",
    "version": "0.0.1",
    "keywords": ["packager", "css", "javascript", "html", "web", "development"],
    "main": "./lib/app.js",
    "bin": {
        "stitch": "./bin/stitch"
    },
    "directories": {
        "lib": "lib"
