        "commander":    ">=0.3.3",
        "glob":         ">=2.10"
    },
    "devDependencies": {
        "expresso": "0.9.2",
        "should": "*"
    },
    "engines": { "node": ">= 0.5.9" },
    "preferGlobal": true,
    "private": true
}